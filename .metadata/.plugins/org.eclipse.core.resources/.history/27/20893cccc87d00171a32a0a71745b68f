package specyfikatory;

public class GetSet {
	
	private int a;
	private String t;
	
	public int getA() {
		return a;
	}
	public void setA(int a) {
		this.a = a;
	}
	public String getT() {
		return t;
	}
	public void setT(String t) {
		this.t = t;
	}
	
	public GetSet(){}
	public GetSet(int a, String t) {
		super();
		this.a = a;
		this.t = t;
	}
	
	
	
}
