package cwiczenie_J68;

public class Punkt3D extends Punkt2D {
	
	int z;

	public Punkt3D(int x, int y, int z) {
		super(x, y);
		this.z = z;
	}

	public Punkt3D() {
	}
	
	public void Przesuniecie(int a, int b, int c){
		x = x + a;
		y = y + b;
		z = z + c;
		System.out.println("Aktualne po³o¿enie (x,y,z): " + this.x + " " + this.y + " " + this.z);
	}

}
