package cwiczenie_J84;

public class Employee extends Person {
	
	private String pensja;

	public String getPensja() {
		return pensja;
	}

	public void setPensja(String pensja) {
		this.pensja = pensja;
	}

	public Employee(String imie, String nazwisko, String pensja) {
		super(imie, nazwisko);
		this.pensja = pensja;
	}
	
}
