package cwiczenie_J65;

public class Magazyn {

	String nazwa;
	int ilosc;
	double cena;

	public Magazyn(){}
	public Magazyn(String produkt, int ilosc, double cena) {
		super();
		this.nazwa = produkt;
		this.ilosc = ilosc;
		this.cena = cena;
	}
	
	public void nazwaProduktu(String a){
		this.nazwa = a;
	}
	
	public void iloscProduktu(int a){
		this.ilosc = a;
	}
	
	public void cenaProduktu(double a){
		this.cena = a;
	}

	public static void main(String[] args) {
		
		

	}

}
