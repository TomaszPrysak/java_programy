package java_dzien_pierwszy_cwiczenia;

public class Cwiczenie_J1 {

	public static void main(String[] args) {

		System.out.print("Pitos쓰w\nMiros쓰w\nZbigniew");
		
		System.out.println("Pitos쓰w");
		
		System.out.print("Miros쓰w\n");
		
		System.out.print("Zbigniew");
		
		System.out.print("Zbigniew" + "Diana");

	}

}
