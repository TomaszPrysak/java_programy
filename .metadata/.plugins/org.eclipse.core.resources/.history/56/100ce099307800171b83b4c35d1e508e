package java_dzien_pierwszy;

public class Zmienna_stala {

	public static void main(String[] args) {
		
		final int STALA_INT = 8; // deklarowanie i inicjalizowanie zmiennej z jednoczesnym s³ówkiem final powoduje, ¿e nie mo¿emy pozniej zmienic jej wartosci 
		STALA_INT = 9;
		
		final int STALA_FLOAT;
		STALA_FLOAT = 8;

	}

}
