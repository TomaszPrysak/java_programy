package cwiczenie_J71;

public class PKO extends KontoBankowe {
	
	@Override
	public void addDebit() {
	}

	@Override
	public void insert() {
	}
	
	public static void main(String[] args) {
		
		PKO kb1 = new PKO();
		
		System.out.println(kb1.saldo);
		
		kb1.addSaldo(3.25f);
		
		System.out.println(kb1.saldo);
		
		kb1.subbSaldo(1f);
		
		System.out.println(kb1.saldo);
		
	}


}
