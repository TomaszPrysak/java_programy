package java_dzien_pierwszy_cwiczenia;

public class Cwiczenie_J12 {

	public static void main(String[] args) {
		
		int a = 21;
		int b = 7;
		
		System.out.println(a+b);
		System.out.println(a-b);
		System.out.println(a*b);
		System.out.println(a/b);
		
		double z = 3.4;
		double x = 7.1;
		
		System.out.println(z+x);
		System.out.println(z-x);
		System.out.println(z*x);
		System.out.println(z/x);
		
	}

}
