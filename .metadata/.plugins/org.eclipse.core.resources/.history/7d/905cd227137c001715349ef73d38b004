package java_dzien_czwarty_cwiczenia;

import java.util.Random;

public class Cwiczenie_J43 {

	public static void main(String[] args) {
		
		int [] tab = new int [10];
		
		for (int i = 1; i <=10; i++){
			Random gen = new Random(2);
			int x = gen.nextInt() - 10;
			tab[i] = x;
		}
		
		System.out.println(tab);
		

	}

}
