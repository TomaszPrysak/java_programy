package obsluga_bledow;

public class ObslugaBledow_J89 {
	
	public static void testString(String s){
		
		try{
			System.out.println(s.length());
		}catch(NullPointerException e){
			e.printStackTrace(); // ta funkja powoduje, ¿e w konsoli wyœwietlamy informacjê o b³êdzie
			System.out.println("Nie mo¿na podaæ d³ugoœci dla napisu - zosta³ przekazany null");
		}
		
	}
	
	public static void main(String[] args) {
		
		testString("Test");
		testString(null);
		
	}

}
