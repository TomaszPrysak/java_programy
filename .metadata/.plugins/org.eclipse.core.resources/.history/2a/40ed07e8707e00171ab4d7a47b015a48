package cwiczenie_67;

public class Tyre extends Part {
	
	int rozmiar;
	int szerokosc;
	int wysokosc;
	
	public Tyre(String producent, String model, String seria, int rozmiar, int szerokosc, int wysokosc) {
		super(producent, model, seria); // super oznacza, ¿e dziedziczymy  podajemy jakie pola z klasy nadrzêdnej dziedziczymy
		this.rozmiar = rozmiar;
		this.szerokosc = szerokosc;
		this.wysokosc = wysokosc;
	}
	
}
